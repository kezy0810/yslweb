    <select id="find" parameterType="java.lang.String" resultType="SysMaxnum" >
		select 
		version,code,notype
        from system_maxnum  where notype = #{notype}
	</select> 
	
	
	
	  <update id="update" parameterType="com.qkl.ztysl.api.po.sys.SysMaxnum" >    
	     update system_maxnum 
	     set code = #{code}
	     where  notype = #{notype}    
